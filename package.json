{
  "name": "veralux-voice-runtime",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "report:redis": "tsx scripts/print_redis_contract.ts",
    "tenantcfg": "tsx scripts/tenantcfg.ts",
    "whisper:replay": "tsx scripts/whisper_replay.ts",
    "test": "tsx --test",
    "build": "tsc -p tsconfig.json && npm run copy-audio-assets",
    "copy-audio-assets": "mkdir -p dist/audio/vendor/amrwb && cp -R src/audio/vendor/amrwb/amrwb.js src/audio/vendor/amrwb/amrwb.wasm dist/audio/vendor/amrwb/"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@roamhq/wrtc": "^0.9.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "ioredis": "^5.8.2",
    "koffi": "^2.15.1",
    "onnxruntime-node": "^1.23.2",
    "opusscript": "^0.1.1",
    "pino": "^9.2.0",
    "prom-client": "^15.1.3",
    "undici": "^7.19.2",
    "ws": "^8.16.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/node": "^20.11.30",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5"
  }
}
